@model Vidly.ViewModels.IndexMoviesViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class="table table-striped table-hover">
<thead>
    <tr>

        <td>Customer Name</td>
    </tr>
</thead>
<tbody>
    @foreach (var movie in Model.Movies)
    {
        <tr>

            <td><a href="/movies/view/@movie.Id">@movie.Name</a> </td>
        </tr>
    }
</tbody>
</table>

